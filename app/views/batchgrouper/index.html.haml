= simple_form_for @batchgrouper, :url => "batchgrouper", :remote => true do |f|
  = render "shared/system", :f => f, :system_id => @batchgrouper.system_id, :house => @batchgrouper.house
  %fieldset
    %legend Data
    = f.input :file, :as => :file
    = f.input :single_group
    #single_group_result
  .right
    = f.submit :class => "button positive"
    #loading