= simple_form_for :webgrouper_patient_case do |f|
  = f.input :sex, :label => "Geschlecht", :as => :radio_buttons, :collection => %w(M W U)
  = f.input :age_years, :label => "Alter", :placeholder => "30"
  = f.date_select :entry_date, :label => "Entry date"
  = f.submit "Gruppieren"
  
%h2 Gruppierung
%p= @result